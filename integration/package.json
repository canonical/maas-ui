{
  "name": "@maas-ui/maas-ui-integration",
  "private": true,
  "version": "3.2.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint \"cypress/**/*.{ts,tsx}\"",
    "lint-package-json": "npmPkgJsonLint .",
    "start": "yarn serve-ui & yarn serve-ui-proxy",
    "cypress-test": "start-server-and-test start localhost:8400 cypress-run",
    "serve-ui": "yarn serve -s ../build -p 8401",
    "serve-ui-proxy": "yarn --cwd ../proxy serve-ui-proxy",
    "serve-frontends": "yarn --cwd ../proxy serve-frontends",
    "serve-base": "yarn --cwd ../proxy serve-base",
    "cypress-run": "yarn cypress run",
    "cypress-run-a11y": "yarn cypress run --config integrationFolder=cypress/integration/accessibility",
    "cypress-open": "yarn cypress open"
  },
  "devDependencies": {
    "@maas-ui/maas-ui-shared": "3.2.0",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "concurrently": "6.4.0",
    "cypress": "9.1.1",
    "cypress-axe": "0.13.0",
    "cypress-wait-until": "1.7.2",
    "dotenv-flow": "3.2.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-no-only-tests": "2.6.0",
    "nanoid": "3.1.30",
    "npm-package-json-lint": "5.4.2",
    "serve": "13.0.2",
    "start-server-and-test": "1.14.0"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  }
}
